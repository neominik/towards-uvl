	EMBEDDED | (EMBEDDED | NAMESPACES) & (!NAMESPACES | !EMBEDDED)
	NAMESPACES & (SYSVIPC | POSIX_MQUEUE) | !IPC_NS
	NAMESPACES & EXPERIMENTAL | !USER_NS
	NAMESPACES & EXPERIMENTAL | !PID_NS
	EMBEDDED | HOTPLUG & !HOTPLUG
	EMBEDDED | PRINTK & !PRINTK
	EMBEDDED | BUG & !BUG
	EMBEDDED | ELF_CORE & !ELF_CORE
	EMBEDDED | BASE_FULL & !BASE_FULL
	EMBEDDED | FUTEX & !FUTEX
	EMBEDDED | EPOLL & !EPOLL
	EMBEDDED | SIGNALFD & !SIGNALFD
	EMBEDDED | TIMERFD & !TIMERFD
	EMBEDDED | AIO & !AIO
	HAVE_PERF_EVENTS | !PERF_COUNTERS
	EMBEDDED | VM_EVENT_COUNTERS & !VM_EVENT_COUNTERS
	EMBEDDED | !SLOB
	MODULES & MODULE_UNLOAD & EXPERIMENTAL | !MODULE_FORCE_UNLOAD
	!INIT_ALL_POSSIBLE