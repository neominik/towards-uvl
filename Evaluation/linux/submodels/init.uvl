	!BROKEN
	HAVE_KERNEL_GZIP | !KERNEL_GZIP
	HAVE_KERNEL_BZIP2 | !KERNEL_BZIP2
	HAVE_KERNEL_LZMA | !KERNEL_LZMA
	(!SYSVIPC | !SYSCTL | SYSVIPC_SYSCTL) & (!SYSVIPC_SYSCTL | SYSVIPC & SYSCTL)
	(!POSIX_MQUEUE | !SYSCTL | POSIX_MQUEUE_SYSCTL) & (!POSIX_MQUEUE_SYSCTL | POSIX_MQUEUE & SYSCTL)
