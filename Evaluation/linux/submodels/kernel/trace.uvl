	TRACING_SUPPORT & FTRACE & FTRACE_STARTUP_TEST | !EVENT_TRACE_TEST_SYSCALLS
	TRACING_SUPPORT & FTRACE & MMIOTRACE | !MMIOTRACE_TEST
	TRACING_SUPPORT & FTRACE & RING_BUFFER | !RING_BUFFER_BENCHMARK